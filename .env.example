# Supabase
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# App URL
NEXT_PUBLIC_APP_URL=http://localhost:3001

# Stripe Keys
STRIPE_SECRET_KEY=sk_test_...
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Price IDs (Create these in Stripe Dashboard)
NEXT_PUBLIC_STRIPE_STARTER_PRICE_ID=price_...
NEXT_PUBLIC_STRIPE_GROWTH_PRICE_ID=price_...
NEXT_PUBLIC_STRIPE_BUSINESS_PRICE_ID=price_...

# Retell AI
RETELL_API_KEY=your-retell-api-key

# Google OAuth (for Google Sheets & Calendar integration)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Sales Agent Webhooks (set these in Retell AI dashboard)
# These URLs will be called by the AI agent during calls
SALES_LOG_WEBHOOK=${NEXT_PUBLIC_APP_URL}/api/webhooks/sales-log
CALLBACK_SCHEDULER_WEBHOOK=${NEXT_PUBLIC_APP_URL}/api/webhooks/callback-scheduler
EMAIL_SENDER_WEBHOOK=${NEXT_PUBLIC_APP_URL}/api/webhooks/send-email
